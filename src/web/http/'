function login() {
	var username = $("#username").val();
	var password = $("#password").val();
	$.post("http://localhost:5000/api/v1.0/login",{username: username, password: btoa(password)}, function(data) {
		console.log(data);
	}, "jsonp"):
}

$( document ).ready(function() {
	$("#login").click(login);
});
